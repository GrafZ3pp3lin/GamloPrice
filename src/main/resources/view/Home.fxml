<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <MenuBar layoutY="2.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <Menu mnemonicParsing="false" text="File">
            <MenuItem fx:id="MenuItem_Open" mnemonicParsing="false" text="Open" />
            <MenuItem fx:id="MenuItem_Save" mnemonicParsing="false" text="Save" />
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
            <MenuItem fx:id="MenuItem_EditGame" mnemonicParsing="false" text="Edit Game" />
            <MenuItem fx:id="MenuItem_Teams" mnemonicParsing="false" text="Teams" />
            <MenuItem fx:id="MenuItem_Timer" mnemonicParsing="false" text="Timer" />
        </Menu>
        <Menu mnemonicParsing="false" text="Game">
            <MenuItem fx:id="MenuItem_Start" mnemonicParsing="false" text="Start" />
            <MenuItem fx:id="MenuItem_End" mnemonicParsing="false" text="End" />
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
            <MenuItem fx:id="MenuItem_About" mnemonicParsing="false" text="About" />
        </Menu>
    </MenuBar>
    <StackPane fx:id="StackPane_Content" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="25.0">
        <BorderPane fx:id="BorderPane_Default" prefHeight="200.0" prefWidth="200.0">
            <center>
                <Label text="Welcome" BorderPane.alignment="CENTER">
                    <font>
                        <Font size="40.0" />
                    </font>
                </Label>
            </center>
        </BorderPane>
        <BorderPane fx:id="BorderPane_Content" prefHeight="200.0" prefWidth="200.0">
            <center>
                <TabPane fx:id="TabPane_Games" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER" />
            </center>
            <bottom>
                <HBox fx:id="HBox_Controls" alignment="CENTER_RIGHT" prefHeight="50.0" spacing="20.0" BorderPane.alignment="CENTER">
                    <Button fx:id="Button_Edit" mnemonicParsing="false" text="Edit" visible="false" />
                    <Button fx:id="Button_Start" alignment="TOP_LEFT" contentDisplay="CENTER" mnemonicParsing="false" text="Start" visible="false" />
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </padding>
                </HBox>
            </bottom>
        </BorderPane>
    </StackPane>
</AnchorPane>
